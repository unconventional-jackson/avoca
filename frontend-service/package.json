{
  "name": "@unconventional-jackson/avoca-frontend",
  "description": "Avoca AI frontend web application",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build:dev": "tsc && vite build --mode dev",
    "build:prod": "tsc && vite build --mode prod",
    "postinstall": "patch-package",
    "start:local": "vite --host",
    "start:dev": "vite --mode dev",
    "start:prod": "vite --mode prod",
    "lint:check:tsc": "tsc --noEmit",
    "lint:check:eslint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:check:prettier": "prettier --check ./**/*.{ts,js,tsx,jsx,graphql,yaml,yml} --no-error-on-unmatched-pattern",
    "lint:fix:prettier": "prettier --write . --no-error-on-unmatched-pattern",
    "test:unit": "echo \"Not implemented for this service\" && exit 0"
